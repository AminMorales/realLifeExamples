FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 3750.0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    stator
    {
        type            compressible::omegaWallFunction;
        value           $internalField;
    }
    
    rotor
    {
        type            compressible::omegaWallFunction;
        value           $internalField;
    }

    #include		"ami.include";
}


// ************************************************************************* //
